{
  "name": "my-better-t-app",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "check-types": "pnpm -r check-types",
    "dev:native": "pnpm --filter native dev",
    "dev:web": "pnpm --filter web dev",
    "dev:server": "pnpm --filter server dev"
  },
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "dotenv": "catalog:",
    "zod": "catalog:",
    "@my-better-t-app/env": "workspace:*"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/node": "^22.13.11",
    "@my-better-t-app/config": "workspace:*"
  }
}
